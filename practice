function countUniqueValues(arr){
  var prev = 1;   
  // add whatever parameters you deem necessary - good luck!
  let el = {};
  for(let i = 0 ; i < arr.length; i++ ) {
      console.log('e', el);
      console.log('i', arr[i]);
      el[arr[i]] = (el[arr[i]] || 0) + 1
  }
  console.log(el)
  return Object.keys(el).length
}
