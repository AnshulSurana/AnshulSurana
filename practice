          console.log('f', a[f[i]]);
          if (!a[f[i]]) {
              a[f[i]] = 1; 
          } else {
              a[f[i]] += 1;
          }
      }
        console.log('a', a);
      for (let j = 0; j < s.length ; j++) {
          console.log('s', a[s[j]]);
          if(!a[s[j]])  {
               return false;
          } else {
              a[s[j]] -=1;
          }
      }
      return true;
      console.log(a);
    }
